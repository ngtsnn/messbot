    "ua-parser-js": "^1.0.37",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/otp-generator": "^4.0.2",
